<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:absControl="chris.calendar.control">
	<Dialog type="Message" id="DialogIDSeries" title="Bitte einloggen..." class="sapUiPopupWithPadding" afterClose="onEntryCancel">
		<content>
			<VBox class="sapUiSmallMargin">
				<Input id="inputUsername" placeholder="Benutzer..."/>
				<Input id="inputPassword" placeholder="Super duper geheimes Passwort..." type="Password"/>
			</VBox>
		</content>
		<buttons>
			<Button icon="sap-icon://accept" type="Accept" press="onOkPress"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>