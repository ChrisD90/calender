<mvc:View xmlns="sap.m" xmlns:f="sap.f" xmlns:card="sap.f.cards" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data" xmlns:grid="sap.ui.layout.cssgrid" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" controllerName="abs.mirahub.controller.Home">
    <Page title="{i18n>labelPageTitle}" floatingFooter="true">
        <headerContent>
            <Image src="theme/img/logo.png" busyIndicatorSize="Auto" height="100%" press="onLogoPress" />
        </headerContent>
        <footer>
            <OverflowToolbar>
                <content>
                    <Image src="theme/img/logo.png" busyIndicatorSize="Auto" height="100%" press="onLogoPress" />
                    <ToolbarSpacer />
                    <Button type="Transparent" text="{i18n>btnContact}" icon="sap-icon://tag-cloud-chart" press="onContactPress" />
                </content>
            </OverflowToolbar>
        </footer>
        <content>
            <layout:Grid>
                <f:Card id="tileJanuary" class="sapUiMediumMarginTop">
                    <f:header>
                        <card:Header title="Januar 2021" subtitle="ab 01.01.2021" iconSrc="sap-icon://validate" />
                    </f:header>
                    <f:content>
                        <Image src="theme/img/logo.png" width="100%" height ="100%"></Image>
                    </f:content>
                </f:Card>
            </layout:Grid>
        </content>
    </Page>
</mvc:View>